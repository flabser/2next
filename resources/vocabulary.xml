<?xml version="1.0" encoding="UTF-8" ?>
<!-- System vocabulary -->
<vocabulary>
	<sentence mode="on" keyword="brand">
		<entry lang="RUS"></entry>
		<entry lang="KAZ"></entry>
		<entry lang="ENG"></entry>
		<entry lang="BG"></entry>
	</sentence>

	<!-- errors -->
	<sentence mode="on" keyword="USER_EXISTS">
		<entry lang="RUS">Пользователь уже зарегестрирован</entry>
		<entry lang="KAZ">Пользователь уже зарегестрирован</entry>
		<entry lang="ENG">User was already registered</entry>
		<entry lang="BG">Потребителят вече е регистриран</entry>
	</sentence>
	<sentence mode="on" keyword="EMAIL_IS_INCORRECT">
		<entry lang="RUS">Неверный электронный почтовый адрес</entry>
		<entry lang="KAZ">Неверный электронный почтовый адрес</entry>
		<entry lang="ENG">Incorrect email</entry>
		<entry lang="BG">Неправилно електронна поща</entry>
	</sentence>
	<sentence mode="on" keyword="WEAK_PASSWORD">
		<entry lang="RUS">Слабый пароль</entry>
		<entry lang="KAZ">Слабый пароль</entry>
		<entry lang="ENG">Weak password</entry>
		<entry lang="BG">Слаба парола</entry>
	</sentence>
	<sentence mode="on" keyword="SERVER_ERROR">
		<entry lang="RUS">Ошибка серевера</entry>
		<entry lang="KAZ">Ошибка серевера</entry>
		<entry lang="ENG">Server error</entry>
		<entry lang="BG">Грешка в сървъра</entry>
	</sentence>
	<sentence mode="on" keyword="USER_NOT_FOUND">
		<entry lang="RUS">Пользователь не найден</entry>
		<entry lang="KAZ">Пользователь не найден</entry>
		<entry lang="ENG">User has not found</entry>
		<entry lang="BG">Потребителят все още не е намерено</entry>
	</sentence>
	
	<!-- warnings -->	
	<sentence mode="on" keyword="VERIFY_EMAIL_SENDING_ERROR">
		<entry lang="RUS">Ошибка при отправке письма с кодом верификации</entry>
		<entry lang="KAZ">Ошибка при отправке письма с кодом верификации</entry>
		<entry lang="ENG">There was error during sending the verification email</entry>
		<entry lang="BG">Имаше грешка при изпращане на имейл за потвърждение</entry>
	</sentence>
	<sentence mode="on" keyword="RESET_PASSWORD_SENDING_ERROR">
		<entry lang="RUS">Ошибка при отправке письма для сброса пароля</entry>
		<entry lang="KAZ">Ошибка при отправке письма для сброса пароля</entry>
		<entry lang="ENG">There was error during sending the email for reset your password</entry>
		<entry lang="BG">Имаше грешка по време на изпращането на имейла за възстановите паролата си</entry>
	</sentence>
	<sentence mode="on" keyword="UNKNOWN_USER_STATUS">
		<entry lang="RUS">Не известный статус пользователя</entry>
		<entry lang="KAZ">Не известный статус пользователя</entry>
		<entry lang="ENG">Unknown user status</entry>
		<entry lang="BG">Неизвестен статус на потребителя</entry>
	</sentence>
	<sentence mode="on" keyword="RESET_PASSWORD_ALREADY_SENT">
		<entry lang="RUS">Пароль сброса был отправлен</entry>
		<entry lang="KAZ"></entry>
		<entry lang="ENG">Reset password has already sent</entry>
		<entry lang="BG"></entry>
	</sentence>
	<sentence mode="on" keyword="INVITATION_SENDING_ERROR">
		<entry lang="RUS">Ошибка при отправке письма с приглашением</entry>
		<entry lang="KAZ">Ошибка при отправке письма с приглашением</entry>
		<entry lang="ENG">There is error during sending a invitation</entry>
		<entry lang="BG">Грешка по време на изпращането на покана</entry>
	</sentence>
	
</vocabulary>
