<?xml version="1.0" encoding="utf-8"?>
<rule type="page" id="send_verify_email" mode="on" anonymous="on">
	<description></description>
	<caching>NO_CACHING</caching>
	<rununderuser source="STATIC" type="TEXT">[observer]</rununderuser>
	<xsltfile>pages/email-verify.xsl</xsltfile>

	<element mode="ON" type="SCRIPT">
		<events>
			<doscript source="java_class">cashtracker.page.SendVerifyEMail</doscript>
		</events>
	</element>
</rule>
