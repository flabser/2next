<?xml version="1.0" encoding="utf-8"?>
<rule type="page" id="password-recovery" mode="on" anonymous="on">
	<description></description>
	<rununderuser source="STATIC" type="TEXT">[observer]</rununderuser>
	<xsltfile>site/password-recovery.xsl</xsltfile>

	<element mode="ON" type="INCLUDED_PAGE">
		<name>welcome-site-inc</name>
		<value>welcome-site-inc</value>
	</element>
	<element mode="ON" type="SCRIPT">
		<name>password-recovery</name>
		<events>
			<doscript source="file">cashtracker.page.app.PasswordRecovery</doscript>
		</events>
	</element>

	<caption name="pwd_recovery_title" />
	<caption name="pwd_recovery_description" />
	<caption name="pwd_recovery_new_pwd" />
	<caption name="pwd_recovery_btn_send" />
	<caption name="pwd_recovery_btn_ok" />
</rule>
