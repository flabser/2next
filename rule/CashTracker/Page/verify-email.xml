<?xml version="1.0" encoding="utf-8"?>
<rule type="page" id="verify-email" mode="on" anonymous="on">
	<description></description>
	<rununderuser source="STATIC" type="TEXT">[observer]</rununderuser>
	<xsltfile>site/verify-email.xsl</xsltfile>

	<element mode="ON" type="SCRIPT">
		<events>
			<doscript source="file">cashtracker.page.app.VerifyEMail</doscript>
		</events>
	</element>
	<element mode="ON" type="INCLUDED_PAGE">
		<name>site-inc</name>
		<value>site-inc</value>
	</element>

	<caption name="verify_email_title" />
	<caption name="verify_email_error" />
	<caption name="verify_email_done" />
	<caption name="verify_email_already" />
</rule>
