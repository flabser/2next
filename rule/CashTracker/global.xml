<?xml version="1.0" encoding="UTF-8"?><rule mode="on" type="global">
	<description>SmartFinance</description>
	<appname>CashTracker</appname>
	<impl>com.flabser.solutions.cashtracker.Database</impl>
	<entrypoint>Provider?type=page&amp;id=welcome</entrypoint>
	<defaultredirecturl>Provider?type=page&amp;id=operations</defaultredirecturl>
	<langs>
		<entry id="RUS">Русский</entry>
		<entry id="ENG">English</entry>
	</langs>

	<roles>
		<entry id="operations">can add transactions</entry>
		<entry id="rn_amount_control">a receiver of notifying about exceeding sum  of cash transaction</entry>
		<entry id="dailymailing">a receiver of notifying about a day transaction</entry>
		<entry id="administrator">administrator</entry>
	</roles>

	<oauth>
		<provider id="vk" mode="ON">
			<auth_uri/>
			<response_type/>
			<scope/>
			<redirect_uri/>
			<icon/>
		</provider>
	</oauth>
</rule>